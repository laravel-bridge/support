{
    "name": "laravel-bridge/support",
    "description": "Support library for Laravel Bridge implements",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "illuminate/container": "^5.1",
        "illuminate/http": "^5.1",
        "psr/container": "^1.0",
        "symfony/psr-http-message-bridge": "^1",
        "zendframework/zend-diactoros": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4 | ^5 | ^6",
        "pimple/pimple": "^3.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "LaravelBridge\\Support\\": "src/Support"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}

{
    "name": "laravel-bridge/support",
    "description": "Support library for Laravel Bridge implements",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "illuminate/container": "^5.6 | ^6 | ^7",
        "illuminate/http": "^5.6 | ^6 | ^7",
        "laminas/laminas-diactoros": "^2.2",
        "php-di/php-di": "^6.0",
        "phpunit/phpunit": "^6 | ^7 | ^8 | ^9",
        "pimple/pimple": "^3.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/psr-http-message-bridge": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelBridge\\Support\\": "src/Support"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}

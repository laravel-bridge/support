{
    "name": "laravel-bridge/support",
    "description": "Support library for Laravel Bridge implements",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "psr/container": "^1.0",
        "illuminate/container": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4 | ^5 | ^6",
        "squizlabs/php_codesniffer": "^3.4",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "LaravelBridge\\Support\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
